<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="FirstColorRGB" R="255" G="255" B="255" A="255" />
    <Color x:Key="SecondColorRGB" R="200" G="200" B="200" A="255" />
    <Color x:Key="TherdColorRGB" R="100" G="100" B="100" A="255"/>
    <Color x:Key="GradientColorRGB" R="0" G="0" B="0" A="255" />
    <Color x:Key="BorderColorRGB" R="100" G="100" B="100" A="255" />
    <Color x:Key="BorderMOverRGB" R="127" G="127" B="127" A="255" />
    <Color x:Key="TextColorFirstRGB" R="0" G="0" B="0" A="255"/>
    <SolidColorBrush x:Key="FirstColor"		Color="{StaticResource FirstColorRGB}"/>
    <SolidColorBrush x:Key="SecondColor"	Color="{StaticResource SecondColorRGB}"/>
    <SolidColorBrush x:Key="TherdColor"		Color="{StaticResource TherdColorRGB}"/>
    <SolidColorBrush x:Key="BorderColor"	Color="{StaticResource BorderColorRGB}"/>
    <SolidColorBrush x:Key="GradientColor"	Color="{StaticResource GradientColorRGB}"/>
    <SolidColorBrush x:Key="BorderMOver"	Color="{StaticResource BorderMOverRGB}"/>
    <SolidColorBrush x:Key="TextColorFirst"	Color="{StaticResource TextColorFirstRGB}"/>
    <RadialGradientBrush x:Key="GradientButtonOver">
    </RadialGradientBrush>
    <RadialGradientBrush x:Key="GradientButtonMenu">
    </RadialGradientBrush>
    <RadialGradientBrush  x:Key="GradientButtonRegularOver">
    </RadialGradientBrush>
    <RadialGradientBrush  x:Key="GradientButtonRegularOverPressed">
    </RadialGradientBrush>
    <RadialGradientBrush x:Key="GradientBorder.MouseOver">
    </RadialGradientBrush>
    <RadialGradientBrush x:Key="GradientBorder.Pressed" >
    </RadialGradientBrush>
    <LinearGradientBrush x:Key="Grid" SpreadMethod="Reflect" StartPoint="0,0" EndPoint="1,1">
    </LinearGradientBrush>
</ResourceDictionary>

